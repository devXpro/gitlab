{
  "name": "@your-scope/your-package",
  "version": "1.0.0",
  "description": "Example package for GitLab Package Registry",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts"
  },
  "keywords": [
    "example",
    "gitlab",
    "package-registry"
  ],
  "author": "Your Name",
  "license": "MIT",
  "publishConfig": {
    "@your-scope:registry": "https://gitlab.local:8080/api/v4/projects/YOUR_PROJECT_ID/packages/npm/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.local:8080/your-group/your-project.git"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.0"
  }
}

